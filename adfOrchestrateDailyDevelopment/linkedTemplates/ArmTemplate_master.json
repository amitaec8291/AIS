{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfOrchestrateDailyDevelopment"
		},
		"AzureAisDataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureAisDataLake'"
		},
		"DKMonitoring_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'DKMonitoring'"
		},
		"AzureAisDataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://amitadls.dfs.core.windows.net/"
		},
		"DKMonitoring_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1215-124615-4a77eb3z"
		},
		"lsWebAIS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://web.ais.dk/aisdata/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfOrchestrateDailyDevelopment_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureAisDataLake_accountKey": {
						"value": "[parameters('AzureAisDataLake_accountKey')]"
					},
					"DKMonitoring_accessToken": {
						"value": "[parameters('DKMonitoring_accessToken')]"
					},
					"AzureAisDataLake_properties_typeProperties_url": {
						"value": "[parameters('AzureAisDataLake_properties_typeProperties_url')]"
					},
					"DKMonitoring_properties_typeProperties_existingClusterId": {
						"value": "[parameters('DKMonitoring_properties_typeProperties_existingClusterId')]"
					},
					"lsWebAIS_properties_typeProperties_url": {
						"value": "[parameters('lsWebAIS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}